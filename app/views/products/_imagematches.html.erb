<div style="overscroll-y:scoll;height:450px;">
<table style="overscroll-y:scoll;">
<tr>
<th colspan="7">Choose a Primary Image - <%= @posproduct.name %> | <%= @posproduct.item_number %></th>
</tr>
<tr>
<th colspan="2"><button type="button" class="update_image" data-src="text" data-item-id="<%= @posproduct.item_id %>" >Update</button></th>
<th colspan="5"><input type="text" id="imgsrc" name="src" placeholder="URL"/><input type="hidden" name="item-id" </th>
</tr>

<tr>
<th colspan="7"> External Searches</th>
</tr>
<tr>
<td colspan="3">
	<a target="_blank" href="https://www.google.ca/search?q=<%=URI.encode @posproduct.name%>&client=ubuntu&hs=aoG&channel=fs&source=lnms&tbm=isch"><button class="tiny">More Images Google</button></a>
</td>
<td colspan="4">
	<a target="_blank" href="http://www.adultdvdmarketplace.com/xcart/adult_dvd/dvd_search.php?type=title&search=<%=URI.encode @posproduct.name%>"><button class="tiny">More Images AdultDVDMarketPlace</button></a>
</td>


</tr>

<tr><th colspan="7"> First Image From Google </th></tr>

<tr>
<td colspan="7">
<a href="<%= @googleimage %>" target="_blank">
<img class="replacementimage" src="<%= @googleimage %>" />
<hr>
<input type="radio" class="update_image" data-src="<%=  @googleimage %>" data-item-id="<%= @posproduct.item_id %>" data-item-number="<%=@posproduct.item_number%>" name="main_image" />

</a>

</td>

</tr>

<% @products_by_like.each do | product | %>
